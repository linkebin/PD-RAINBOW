<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:activiti="http://activiti.org/bpmn"
             typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath"
             targetNamespace="http://www.activiti.org/test">
	<process id="launchProcess" name="活动申请" isExecutable="true">
		<startEvent id="start" name="流程开始">
			<extensionElements>
				<activiti:formProperty id="ID_S" name="活动ID" type="string"
				                       required="true"></activiti:formProperty>
				<activiti:formProperty id="NAME_S" name="活动名称" type="string"
									   required="true"></activiti:formProperty>
			</extensionElements>
		</startEvent>
		<userTask id="ApprovalTask" name="活动审批"
				  activiti:candidateUsers="${activityService.findUsers(execution)}" isForCompensation="true">
			<extensionElements>
				<activiti:formProperty id="launchApproved" name="launch Approved" type="enum">
					<activiti:value id="true" name="通过"></activiti:value>
					<activiti:value id="false" name="打回"></activiti:value>
				</activiti:formProperty>
			</extensionElements>
		</userTask>
		<endEvent id="end" name="流程结束"></endEvent>
		<sequenceFlow id="flow1" sourceRef="start" targetRef="ApprovalTask"></sequenceFlow>
		<sequenceFlow id="flow2" sourceRef="ApprovalTask" targetRef="AutoTask"></sequenceFlow>
		<serviceTask id="AutoTask" name="Auto Task"
					 activiti:expression="${activityService.launch(execution)}"></serviceTask>
		<sequenceFlow id="flow3" sourceRef="AutoTask" targetRef="end"></sequenceFlow>
	</process>

</definitions>